<?xml version="1.0" encoding="UTF-8"?>
<uid:Application xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:uid="http://uid/1.0">
  <hasViews>
    <hasStage name="ViewA">
      <hasButton name="ButtonStage2">
        <hasEvents name="touch" sendsNotification="//@hasNotifications/@hasNotification.1"/>
      </hasButton>
    </hasStage>
    <hasStage name="ViewB">
      <hasButton name="ButtonStage1">
        <hasEvents name="touch" sendsNotification="//@hasNotifications/@hasNotification.0"/>
      </hasButton>
    </hasStage>
    <hasMediator name="ApplicationMediator">
      <hasNotificationHandler recievesNotification="//@hasNotifications/@hasNotification.4"/>
      <hasNotificationHandler recievesNotification="//@hasNotifications/@hasNotification.5"/>
    </hasMediator>
    <hasMediator name="ViewAMediator">
      <hasNotificationHandler recievesNotification="//@hasNotifications/@hasNotification.1" sendsNotification="//@hasNotifications/@hasNotification.3"/>
    </hasMediator>
    <hasMediator name="ViewBMediator">
      <hasNotificationHandler recievesNotification="//@hasNotifications/@hasNotification.0" sendsNotification="//@hasNotifications/@hasNotification.2"/>
    </hasMediator>
  </hasViews>
  <hasControllers>
    <hasCommand name="State1Command" hasNotification="//@hasNotifications/@hasNotification.4"/>
    <hasCommand name="State2Command" hasNotification="//@hasNotifications/@hasNotification.5"/>
  </hasControllers>
  <hasNotifications>
    <hasNotification name="toState1"/>
    <hasNotification name="toState2"/>
    <hasNotification name="toState1Notification"/>
    <hasNotification name="toState2Notification"/>
    <hasNotification name="showState1" recievedByCommand="//@hasControllers/@hasCommand.0"/>
    <hasNotification name="showState2" recievedByCommand="//@hasControllers/@hasCommand.1"/>
  </hasNotifications>
</uid:Application>
